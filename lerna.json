{
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "publishConfig": {
        "access": "public"
      },
      "ignoreChanges": [
        "*.md",
        "*.json",
        "*.txt",
        "test/**",
        "example/**",
        "package.json"
      ]
    },
    "bootstrap": {
      "npmClient": "npm"
    }
  },
  "npmClient": "npm",
  "version": "1.0.8"
}
